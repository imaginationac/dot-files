[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	hist = log --pretty=format:\"%Cgreen%h%Creset %ad | %s%d %Cblue[%an]\" --graph --date=short
	type = cat-file -t
	dump = cat-file -p
[user]
	name = Dorian Patterson
	email = imaginationac@gmail.com
[core]
	editor = "gvim -f"
	autocrlf = input
	safecrlf = true
[color]
	ui = true
[merge]
	tool = gvimdiff
[credential]
	helper = gnome-keyring
[push]
	default = simple
